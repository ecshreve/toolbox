{
	"name": "toolbox-dev",
	"build": {
		"dockerfile": "./Dockerfile",
		"context": ".."
	},
	"features": {
		"ghcr.io/devcontainers/features/docker-outside-of-docker:1": {}
	},
	"customizations": {
		"vscode": {
			"extensions": [
				"redhat.ansible",
				"ms-azuretools.vscode-docker",
				"ms-python.python",
				"ms-python.vscode-pylance",
				"ms-toolsai.jupyter",
				"golang.go",
				"hashicorp.hcl"
			]
		}
	}
}