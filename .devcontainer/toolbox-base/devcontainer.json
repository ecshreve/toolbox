{
	"name": "base",
	"build": {
		"cacheFrom": "ghcr.io/ecshreve/toolbox-base:latest",
		"dockerfile": "./Dockerfile",
		"context": ".",
		"args": {
			"DOCKER_GID": "999"
		}
	},
	"features": {
		"ghcr.io/devcontainers/features/docker-outside-of-docker:1": {}
	},
	"customizations": {
		"vscode": {
			"extensions": [
				"redhat.ansible",
				"ms-azuretools.vscode-docker",
				"ms-python.python",
				"ms-python.vscode-pylance",
				"ms-toolsai.jupyter",
				"golang.go",
				"hashicorp.hcl"
			]
		}
	}
}